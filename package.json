{
  "name": "mygimp-monorepo",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm --filter client dev\" \"pnpm --filter server dev\"",
    "install:all": "pnpm install --recursive"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
